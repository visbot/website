{
  "name": "@visbot/website",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "bootstrap": "node tools/bootstrap.mjs",
    "build": "vite build",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "dev": "npm-run-all --parallel dev:*",
    "dev:lambda": "netlify dev",
    "dev:web": "vite dev --open",
    "package": "vite package",
    "postinstall": "npm run bootstrap",
    "prepare": "husky install",
    "preview": "vite preview --open"
  },
  "dependencies": {
    "@visbot/file_id.diz": "^1.0.0",
    "feed": "^4.2.2",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@lufrai/modern-css-reset": "^1.2.0",
    "@sveltejs/adapter-netlify": "2.0.8",
    "@sveltejs/adapter-static": "^2.0.2",
    "@sveltejs/kit": "^1.22.3",
    "@tsconfig/svelte": "^5.0.0",
    "@typescript-eslint/eslint-plugin": "^6.1.0",
    "@typescript-eslint/parser": "^6.1.0",
    "autoprefixer": "^10.4.14",
    "aws-lambda": "^1.0.7",
    "degit": "^2.8.4",
    "eslint": "^8.45.0",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.2.3",
    "netlify-cli": "^15.9.0",
    "npm-run-all2": "^6.0.6",
    "postcss": "^8.4.26",
    "prettier": "^2.8.8",
    "prettier-plugin-svelte": "^2.10.1",
    "prettier-plugin-tailwindcss": "^0.3.0",
    "shelljs": "^0.8.5",
    "svelte": "^4.0.5",
    "svelte-check": "^3.4.6",
    "svelte-preprocess": "^5.0.4",
    "tailwindcss": "^3.3.3",
    "tslib": "^2.6.0",
    "turbo": "^1.10.8",
    "typescript": "^5.1.6",
    "vite": "^4.4.4"
  },
  "lint-staged": {
    "*.svelte": "prettier --write --plugin-search-dir=. --plugin=prettier-plugin-tailwindcss ./src/**/*.svelte",
    "*.(ts|js)": [
      "eslint --cache --fix",
      "prettier --write"
    ],
    "*.(css|scss)": [
      "stylelint --cache --fix",
      "prettier --write"
    ]
  }
}
