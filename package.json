{
  "name": "@visbot/website",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "bootstrap": "node tools/bootstrap.mjs",
    "build": "vite build",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "dev": "vite dev",
    "package": "vite package",
    "postinstall": "npm run bootstrap",
    "prepare": "husky install",
    "preview": "vite preview",
    "start": "vite dev"
  },
  "devDependencies": {
    "@lufrai/modern-css-reset": "^1.2.0",
    "@sveltejs/adapter-netlify": "1.0.0-next.76",
    "@sveltejs/adapter-static": "next",
    "@sveltejs/kit": "next",
    "@tsconfig/svelte": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^5.42.0",
    "@typescript-eslint/parser": "^5.42.0",
    "autoprefixer": "^10.4.13",
    "degit": "^2.8.4",
    "eslint": "^8.26.0",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "postcss": "^8.4.18",
    "prettier": "^2.7.1",
    "prettier-plugin-svelte": "^2.8.0",
    "prettier-plugin-tailwindcss": "^0.1.13",
    "shelljs": "^0.8.5",
    "svelte": "^3.52.0",
    "svelte-check": "^2.9.2",
    "svelte-preprocess": "^4.10.7",
    "tailwindcss": "^3.2.1",
    "tslib": "^2.4.1",
    "typescript": "^4.8.4",
    "vite": "^3.2.2"
  },
  "lint-staged": {
    "*.svelte": "prettier --write --plugin-search-dir=. --plugin=prettier-plugin-tailwindcss ./src/**/*.svelte",
    "*.(ts|js)": [
      "eslint --cache --fix",
      "prettier --write"
    ],
    "*.(css|scss)": [
      "stylelint --cache --fix",
      "prettier --write"
    ]
  },
  "dependencies": {
    "@visbot/file_id.diz": "^1.0.0"
  }
}
