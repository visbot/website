{
  "name": "@visbot/website",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "bootstrap": "node tools/bootstrap.mjs",
    "build": "vite build",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "dev": "npm-run-all --parallel dev:*",
    "dev:lambda": "netlify dev",
    "dev:web": "vite dev",
    "package": "vite package",
    "postinstall": "npm run bootstrap",
    "prepare": "husky install",
    "preview": "vite preview"
  },
  "dependencies": {
    "@lukeed/uuid": "^2.0.0",
    "@visbot/file_id.diz": "^1.0.0",
    "cross-fetch": "^3.1.5"
  },
  "devDependencies": {
    "@lufrai/modern-css-reset": "^1.2.0",
    "@sveltejs/adapter-netlify": "2.0.5",
    "@sveltejs/adapter-static": "^2.0.1",
    "@sveltejs/kit": "^1.10.0",
    "@tsconfig/svelte": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^5.54.0",
    "@typescript-eslint/parser": "^5.54.0",
    "autoprefixer": "^10.4.8",
    "degit": "^2.8.4",
    "eslint": "^8.35.0",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.1.2",
    "netlify-cli": "^13.2.1",
    "npm-run-all2": "^6.0.4",
    "postcss": "^8.4.16",
    "prettier": "^2.8.4",
    "prettier-plugin-svelte": "^2.7.0",
    "prettier-plugin-tailwindcss": "^0.2.4",
    "shelljs": "^0.8.5",
    "svelte": "^3.44.0",
    "svelte-check": "^3.0.4",
    "svelte-preprocess": "^5.0.1",
    "tailwindcss": "^3.2.7",
    "tslib": "^2.5.0",
    "typescript": "^4.9.5",
    "vite": "^4.1.4"
  },
  "lint-staged": {
    "*.svelte": "prettier --write --plugin-search-dir=. --plugin=prettier-plugin-tailwindcss ./src/**/*.svelte",
    "*.(ts|js)": [
      "eslint --cache --fix",
      "prettier --write"
    ],
    "*.(css|scss)": [
      "stylelint --cache --fix",
      "prettier --write"
    ]
  }
}
